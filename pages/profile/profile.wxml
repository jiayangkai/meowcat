<view class='container'>
  <!-- user -->
  <view class='user'>
    <image src='{{user.url}}' class='userinfo-avatar' bacground-size='cover'></image>
    <view>{{user.name}}</view>
    <view>毛积分：{{user.points}}</view>
  </view>
  <!--setting  -->
  <view class='navigator_bar'>
    <view class="weui-cells weui-cells_after-title">
      <view wx:for='{{navigatorbarlist}}'>
        <navigatorbar navigator_url='{{item.navigatorurl}}' message='{{item.message}}' imgurl='{{item.imgurl}}'/>
      </view>
    </view>
  </view>
  <view>
    <navigator url="../timeline/timeline" open-type="redirect">测试</navigator>
  </view>
</view>